{
  "version": 2,
  "name": "ms-shared-samtykke",
  "scope": "minelev",
  "alias": [
    "samtykker.api.minelev.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@db-tfkcosmos-connection",
    "MONGODB_COLLECTION": "samtykker",
    "MONGODB_NAME": "minelev",
    "MOA_TENANT_ID": "@moa-tenant-id",
    "JWT_SECRET": "@minelev-jwt-secret",
    "PAPERTRAIL_HOST": "@tfk-papertrail-host",
    "PAPERTRAIL_PORT": "@tfk-papertrail-port",
    "PAPERTRAIL_HOSTNAME": "elevpc"
  },
  "builds": [
    {
      "src": "/api/*.js",
      "use": "@now/node"
    },
    { 
      "src": "/index.js",
      "use": "@now/node" 
    },
    {
      "src": "*.md",
      "use": "@now/md"
    }
  ],
  "routes": [
    { "src": "/docs",
      "dest": "/README.html"
    },
    { "src": "/favicon.ico",
      "dest": "/static/favicon.ico"
    },
    {
      "src": "/stats/total",
      "dest": "/api/stats-total.js"
    },
    {
      "src": "/stats/signed",
      "dest": "/api/stats-signed.js"
    },
    {
      "src": "/stats/declined",
      "dest": "/api/stats-declined.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "engines": {
    "node": ">= 10.16.0"
  }
}
